{
    "name": "eagle-plugin-pinterest-search",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vite build -c vite.config.js --outDir dist/build",
        "build-win": "vite build -c vite.config.win.js --outDir dist/build-win",
        "build-mac": "vite build -c vite.config.mac.js --outDir dist/build-mac",
        "preview": "vite preview",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "debug": "vite build --watch --force",
        "debug-win": "vite build --watch --force -c vite.config.win.js --outDir dist/build-win",
        "debug-mac": "vite build --watch --force -c vite.config.mac.js --outDir dist/build-mac"
    },
    "dependencies": {
        "@imengyu/vue3-context-menu": "^1.3.3",
        "async": "^3.2.5",
        "element-plus": "^2.3.14",
        "vue": "^3.3.4",
        "vue-mousetrap": "^1.0.5",
        "vue-tippy": "^6.3.1"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.3.3",
        "@vitejs/plugin-vue": "^4.3.4",
        "@vue/eslint-config-prettier": "^8.0.0",
        "eslint": "^8.49.0",
        "eslint-plugin-vue": "^9.17.0",
        "fast-glob": "^3.3.1",
        "prettier": "^3.0.3",
        "rollup-plugin-copy": "^3.5.0",
        "sass": "^1.68.0",
        "unplugin-auto-import": "^0.16.6",
        "unplugin-vue-components": "^0.25.2",
        "vite": "^4.4.9"
    }
}
