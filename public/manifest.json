{
    "id": "exif-metadata",
    "version": "1.0.0",
    "platform": "all",
    "arch": "all",
    "name": "{{manifest.app.name}}",
    "logo": "/logo.png",
    "keywords": ["exif"],
    "fallbackLanguage": "en",
    "languages": ["en", "zh_TW", "zh_CN", "ja_JP", "ko_KR", "es_ES", "de_DE", "ru_RU"],
    "devTools": true,
    "preview": {
        "arw,avif,cr2,cr3,crw,dcp,dng,exv,heic,heif,jp2,jpeg,jpg,jxl,mrw,nef,orf,pef,pgf,psd,raf,rw2,sr2,srw,tiff,tif,webp": {
            "inspector": {
                "multiSelect": false,
                "path": "index.html",
                "height": 32
            }
        }
    }
}
